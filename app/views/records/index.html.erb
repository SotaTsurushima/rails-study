<table>
  <thead>
    <tr>
      <th>isbn</th>
      <th>title</th>
      <th>price</th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
    <tr>
      <td><%= book.isbn %></td>
      <td><%= book.title %></td>
      <td><%= book.price %></td>
      <td><%= book.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>